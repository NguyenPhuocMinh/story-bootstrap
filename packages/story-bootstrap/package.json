{
  "name": "story-bootstrap",
  "version": "0.1.24",
  "main": "lib/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "author": "Minh Nguyá»…n",
  "description": "Frontend for React using ES6 and MUI",
  "homepage": "https://github.com/NguyenPhuocMinh/story-bootstrap#readme",
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es6",
    "watch": "tsc --outDir esm --module es6 --watch"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@mui/styles": "^5.3.0",
    "@testing-library/react": "^12.1.3",
    "formik": "^2.2.9",
    "react-country-flag": "^3.0.2",
    "react-error-boundary": "^3.1.4",
    "react-number-format": "^4.9.3",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sb-core": "^0.1.15",
    "sb-material-ui": "^0.1.24",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
