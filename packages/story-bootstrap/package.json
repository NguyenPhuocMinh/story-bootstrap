{
  "name": "story-bootstrap",
  "version": "0.1.0",
  "main": "lib/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "author": "Minh Nguyá»…n",
  "description": "Frontend for React using ES6 and MUI",
  "homepage": "https://github.com/NguyenPhuocMinh/story-bootstrap#readme",
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es6",
    "watch": "tsc --outDir esm --module es6 --watch"
  },
  "dependencies": {
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@mui/styles": "^5.2.3",
    "formik": "^2.2.9",
    "yup": "^0.32.11",
    "sb-core": "../sb-core",
    "sb-material-ui": "../sb-material-ui",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
